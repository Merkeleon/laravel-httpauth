{
  "name": "merkeleon/laravel-httpauth",
  "description": "Helps to lock develop version with http-auth",
  "keywords": ["laravel","http auth","auth", "basic auth"],
  "license": "MIT",
  "require": {
    "php": ">=5.3.0",
    "illuminate/support": "5.*|6.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.5.*"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Merkeleon\\Laravel\\HttpAuth\\Providers\\HttpAuthServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Merkeleon\\Laravel\\HttpAuth\\": "src/"
    }
  }
}
